{
  "name": "things-ive-seen-ui",
  "version": "0.1.0",
  "description": "UI for The Things I've Seen",
  "repository": "gitlab:boxwhine/the-things-ive-seen",
  "private": false,
  "author": "Jesse Rinehart",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.28",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link-http": "^1.5.11",
    "axios": "^0.18.0",
    "connected-react-router": "^4.5.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/compiled",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/compiled/ --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "lint": "eslint --ext .js src/"
  },
  "proxy": "http://server:8080"
}
