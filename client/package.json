{
  "name": "the-things-ive-seen-ui",
  "version": "0.1.0",
  "description": "UI for The Things I've Seen",
  "repository": {
    "type": "git",
    "url": "https://github.com/boxwhine/the-things-ive-seen.git",
    "directory": "client/"
  },
  "private": false,
  "author": "Jesse Rinehart",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.124",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "apollo-boost": "^0.1.28",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "connected-react-router": "^4.5.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "material-ui-search-bar": "^1.0.0-beta.13",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-load-script": "0.0.6",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/compiled",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/compiled/ --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "lint": "eslint --ext .js src/"
  },
  "proxy": "http://server:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
